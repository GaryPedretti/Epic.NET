---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>epic.tesio.it</title>
  <link href="http://epic.tesio.it/"/>
  <link type="application/atom+xml" rel="self" href="http://epic.tesio.it/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://epic.tesio.it/</id>
  <author>
    <name>Giacomo Tesio</name>
    <email>giacomo@tesio.it</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://epic.tesio.it{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://epic.tesio.it{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
        <name>Giacomo Tesio</name>
        <email>giacomo@tesio.it</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
